<%= form_with url: code_doc_generators_generate_path, method: :post, local: true, data: { turbo: false } do |f| %>
  <div>
    <label>Enter your code here :</label><br>
    <%= f.text_area :code, rows: 20, cols: 80, value: params[:code], required: true %>
  </div>
  <%= f.submit 'submit' %>
<% end %>

<% if @result.present? %>
  <h2>Generated Code Documentation :</h2>
  <% @result.each do |t|%>
    <div><%= t %></div>
  <% end %>
<% end %>
